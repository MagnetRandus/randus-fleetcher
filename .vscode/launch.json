{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "WB",
      "type": "msedge",
      "request": "launch",
      "url": "https://nodomain.sharepoint.com/sites/fleet/_layouts/workbench.aspx?env=Embedded&debug=true&noredir=true&debugManifestsFile=https://localhost:4321/temp/manifests.js",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///.././src/*": "${webRoot}/src/*",
        "webpack:///../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../../src/*": "${webRoot}/src/*"
      },
      "runtimeArgs": ["--remote-debugging-port=9222"]
    },
    {
      "name": "GoBug",
      "type": "msedge",
      "request": "launch",
      "url": "https://nodomain.sharepoint.com/sites/fleet/SitePages/FleetCher.aspx?env=Embedded&debugManifestsFile=https%3A%2F%2Flocalhost%3A4321%2Ftemp%2Fmanifests.js&loadSPFX=true&customActions=%7B%2248352a2f-a243-4f39-838a-de8e08c00be0%22%3A%7B%22location%22%3A%22ClientSideExtension.ApplicationCustomizer%22%2C%22properties%22%3A%7B%22Bottom%22%3A%22Fleet+Management%22%7D%7D%7D",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/src/*",
        "webpack:///.././src/*": "${webRoot}/src/*",
        "webpack:///../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../src/*": "${webRoot}/src/*",
        "webpack:///../../../../../src/*": "${webRoot}/src/*"
